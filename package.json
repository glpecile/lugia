{
    "name": "lugia",
    "main": "index.js",
    "version": "0.0.1",
    "scripts": {
        "dev": "expo start -c --ios",
        "dev:web": "expo start -c --web",
        "dev:android": "expo start -c --android",
        "android": "expo start -c --android",
        "ios": "expo start -c --ios",
        "web": "expo start -c --web",
        "clean": "rm -rf .expo node_modules",
        "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
        "add-post": "bun run ./scripts/add-post.js",
        "up-biome": "bun update @biomejs/biome --latest && bunx @biomejs/biome migrate --write"
    },
    "dependencies": {
        "@bacons/mdx": "^0.4.0",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-navigation/native": "^7.0.0",
        "@rn-primitives/avatar": "~1.1.0",
        "@rn-primitives/portal": "~1.1.0",
        "@rn-primitives/progress": "~1.1.0",
        "@rn-primitives/slot": "^1.1.0",
        "@rn-primitives/tooltip": "~1.1.0",
        "@rn-primitives/types": "^1.1.0",
        "caniuse-lite": "^1.0.30001690",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "expo": "^52.0.7",
        "expo-dev-client": "~5.0.6",
        "expo-linking": "~7.0.2",
        "expo-navigation-bar": "~4.0.3",
        "expo-router": "~4.0.4",
        "expo-splash-screen": "~0.29.10",
        "expo-status-bar": "~2.0.0",
        "expo-system-ui": "~4.0.3",
        "lucide-react-native": "^0.378.0",
        "nativewind": "^4.1.23",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.5",
        "react-native-reanimated": "~3.16.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "^4.0.0",
        "react-native-svg": "15.8.0",
        "react-native-web": "~0.19.13",
        "tailwind-merge": "^2.2.1",
        "tailwindcss": "3.3.5",
        "tailwindcss-animate": "^1.0.7",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@biomejs/biome": "1.9.4",
        "@types/react": "~18.3.12",
        "typescript": "^5.3.3"
    },
    "private": true,
    "expo": {
        "doctor": {
            "reactNativeDirectoryCheck": {
                "exclude": [
                    "lucide-react-native",
                    "@bacons/mdx",
                    "@rn-primitives/avatar",
                    "@rn-primitives/portal",
                    "@rn-primitives/progress",
                    "@rn-primitives/slot",
                    "@rn-primitives/tooltip",
                    "@rn-primitives/types",
                    "caniuse-lite",
                    "class-variance-authority",
                    "tailwindcss",
                    "tailwindcss-animate"
                ]
            }
        }
    }
}
